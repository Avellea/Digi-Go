[audio]

driver/driver="Dummy"

[display]

window/size/resizable=false

[dotnet]

project/assembly_name="GCom"

[filesystem]

import/blender/enabled=false

[rendering]

renderer/rendering_method="mobile"
textures/vram_compression/import_etc2_astc=true
